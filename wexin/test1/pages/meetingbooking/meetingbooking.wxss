.search {
  display: flex;
  align-items: center;
  background-color: #b02923;
  height: 80rpx;
  color: #b02923;
  font-size: 30rpx;
}

.search .date_item {
  width: 35vw;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 0 10rpx;
  margin-left: 10rpx;
  box-sizing: border-box;
}

.search .time_item {
  width: 30vw;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 0 10rpx;
  margin-left: 30rpx;
  box-sizing: border-box;
}

.search .scan_item {
  margin-left: auto;
  margin-right: 40rpx;
}

.type_warp {
  font-size: 35rpx;
  margin: 20rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #ccc;
  padding-bottom: 20rpx;
}

.img_warp {
  display: flex;
  justify-content: center;
  align-items: center;
}

.img_warp .meeting_img {
  margin: 50rpx 0;
  width: 80vw;
}

.submit {
  position: fixed;
  bottom: 0;
  width: 100vw;
}
