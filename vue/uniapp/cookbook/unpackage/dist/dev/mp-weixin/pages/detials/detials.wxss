.header {
  background-color: #ee7b2d;
  width: 750rpx;
  height: 250rpx;
  position: fixed;
  top: 0;
  z-index: 999;
}
.header .myheader {
  width: 100%;
  height: 170rpx;
}
.header .icon_warp {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: flex-end;
}
.header .icon_warp .u-icon {
  margin-right: 20rpx;
}
.banner {
  width: 750rpx;
  height: 686rpx;
  margin-top: 250rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.banner .banner_img {
  width: 750rpx;
  height: 386rpx;
}
.banner .banner_btn {
  width: 686rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #f0c379;
  margin-top: 28rpx;
  margin-bottom: 52rpx;
  border-radius: 16rpx;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
}
.banner .menu_title {
  color: #2c2c2c;
  font-size: 32rpx;
}
.banner .mypageview {
  margin-top: 32rpx;
  margin-bottom: 42rpx;
}
.banner .overlay {
  width: 750rpx;
  height: 385rpx;
  background-color: rgba(0, 0, 0, 0.3);
  position: absolute;
  top: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-size: 32rpx;
}
.banner .overlay .overlay_title {
  margin-top: 88rpx;
  margin-bottom: 32rpx;
}
.banner .overlay .overlay_btn {
  width: 224rpx;
  height: 68rpx;
  line-height: 68rpx;
  background-color: #edd6b2;
  border-radius: 34rpx;
  color: #76401f;
  font-size: 32rpx;
}
.banner .overlay .go_login {
  display: flex;
  margin-top: 36rpx;
}
.content_warp {
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content_warp .comprehend_warp {
  width: 686rpx;
  padding-bottom: 30rpx;
}
.content_warp .comprehend_warp .comprehend_title {
  color: #2c2c2c;
  font-size: 28rpx;
  font-weight: 700;
  margin: 24rpx 0 36rpx 0;
}
.content_warp .comprehend_warp .comprehend_text {
  color: #686868;
  font-size: 28rpx;
}
.content_warp .ingredients_warp {
  width: 686rpx;
  margin-top: 40rpx;
}
.content_warp .ingredients_warp .ingredients_grade {
  display: flex;
  justify-content: center;
  align-items: center;
}
.content_warp .ingredients_warp .ingredients_grade .time {
  color: #686868;
  font-size: 24rpx;
  margin: 0 40rpx 0 12rpx;
}
.content_warp .ingredients_warp .ingredients_grade .star_warp {
  width: 36rpx;
  height: 36rpx;
}
.content_warp .ingredients_warp .ingredients_grade .star_warp .top {
  width: 100%;
  height: 18rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.content_warp .ingredients_warp .ingredients_grade .star_warp .bottom {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 18rpx;
}
.content_warp .ingredients_warp .ingredients_grade .grade {
  color: #686868;
  font-size: 24rpx;
  margin-left: 14rpx;
}
.content_warp .ingredients_warp .table_title {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 686rpx;
  font-size: 28rpx;
  margin: 34rpx 0;
}
.content_warp .ingredients_warp .table_title .add_buy {
  color: #ee7b2d;
}
.content_warp .ingredients_warp .course_warp {
  width: 686rpx;
  margin-top: 46rpx;
  border-bottom: 1rpx solid #dfdfdf;
}
.content_warp .ingredients_warp .course_warp .course_item {
  width: 100%;
  color: #686868;
  font-size: 28rpx;
  margin-bottom: 48rpx;
}
.content_warp .ingredients_warp .course_warp .course_item .course_title {
  color: #2c2c2c;
  font-weight: 700;
}
.content_warp .ingredients_warp .course_warp .course_item .course_img {
  width: 100%;
  height: 440rpx;
  margin: 26rpx 0 34rpx 0;
}
.content_warp .tips_warp {
  width: 686rpx;
  color: #686868;
  font-size: 24rpx;
  margin-top: 40rpx;
  margin-bottom: 46rpx;
  background-color: #eaeaea;
  padding-bottom: 30rpx;
  border-radius: 8rpx;
}
.content_warp .tips_warp .tips_title {
  color: #2c2c2c;
  font-size: 28rpx;
  font-weight: 700;
  margin: 46rpx 0 28rpx 40rpx;
}
.content_warp .tips_warp .tips_text {
  margin: 0 0 18rpx 40rpx;
}
.content_warp .correlation_menu {
  width: 686rpx;
}
.content_warp .correlation_menu .correlation_title {
  margin-bottom: 26rpx;
  font-size: 28rpx;
  color: #2c2c2c;
  font-weight: 700;
}
.content_warp .correlation_menu .correlation_mymenu {
  width: 334rpx;
  margin-bottom: 40rpx;
}

