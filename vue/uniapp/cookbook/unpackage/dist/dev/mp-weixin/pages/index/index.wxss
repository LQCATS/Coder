.header {
  width: 750rpx;
  height: 220rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  position: relative;
  border: 1rpx solid transparent;
  box-sizing: border-box;
}
.header .header_title {
  width: 240rpx;
  color: #fff;
  font-size: 40rpx;
  margin-left: 12rpx;
  margin-top: 78rpx;
}
.header .header_title_msg {
  color: #fff;
  font-size: 22rpx;
  margin-left: 12rpx;
  letter-spacing: 2rpx;
}
.header .header_search {
  height: 74rpx;
  width: 750rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: -35rpx;
  z-index: 9999;
}
.banner {
  width: 750rpx;
  height: 304rpx;
  margin-top: 62rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.banner .swiper {
  width: 686rpx;
  height: 304rpx;
}
.banner .swiper-item {
  display: block;
  width: 686rpx;
  height: 304rpx;
  background-color: yellow;
}
.uni-swiper-dots-horizontal {
  bottom: 0px;
}
.banner_image {
  width: 100%;
  border-radius: 16rpx;
}
.nav {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 32rpx;
}
.nav .myicon {
  margin: 0 80rpx 28rpx 0;
}
.nav .myicon:nth-child(5n) {
  margin-right: 0;
}
.content_warp {
  width: 100%;
  display: flex;
  justify-content: center;
  margin-top: 10rpx;
}
.content_warp .recommend_left {
  width: 334rpx;
  height: 362rpx;
  background-color: skyblue;
  border-radius: 8rpx;
  margin-right: 20rpx;
}
.content_warp .recommend_right {
  width: 332rpx;
  height: 362rpx;
}
.content_warp .recommend_right .right {
  width: 100%;
  height: 176rpx;
  background-color: yellow;
  border-radius: 8rpx;
  margin-bottom: 10rpx;
}
.content_warp .recommend_right .right_bottom {
  margin-bottom: 0;
}

