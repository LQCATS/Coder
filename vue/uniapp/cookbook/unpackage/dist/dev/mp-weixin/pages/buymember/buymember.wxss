}
.member_use_msg_warp .member_use_msg .use_msg_text {
  color: #b7b7b7;
  font-size: 28rpx;
}
.popup_title {
  font-size: 32rpx;
  font-weight: 700;
  color: #5a5a5a;
  text-align: center;
  margin: 22rpx 0 40rpx 0;
}
