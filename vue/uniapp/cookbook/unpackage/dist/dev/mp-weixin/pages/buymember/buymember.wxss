.header {
  width: 750rpx;
  height: 508rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.header .fixed_header {
  width: 750rpx;
  height: 150rpx;
  background-color: #313131;
  border-bottom: 1rpx solid #dbdbdb;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  position: fixed;
  top: 0;
  z-index: 999;
}
.header .fixed_header .header_title_warp {
  height: 50rpx;
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
  margin-left: 34rpx;
}
.header .fixed_header .header_title_warp .header_title {
  color: #fff;
  font-size: 36rpx;
  font-weight: 700;
  margin-left: 252rpx;
}
.header .allmember {
  width: 100%;
  height: 48rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 170rpx;
}
.header .allmember .member_num {
  width: 434rpx;
  height: 100%;
  background-color: rgba(92, 92, 92, 0.7);
  border-radius: 0rpx 40rpx 40rpx 0rpx;
  display: flex;
  align-items: center;
}
.header .allmember .member_num .member_num_msg {
  color: #fff;
  font-size: 24rpx;
  margin-left: 14rpx;
}
.header .allmember .buy {
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  margin-right: 40rpx;
  font-size: 28rpx;
  color: #fff;
}
.member_img {
  height: 100%;
  display: flex;
  align-items: center;
  margin-left: 24rpx;
}
.member_card {
  width: 750rpx;
  height: 302rpx;
  margin-top: 28rpx;
  background: url(http://localhost:4000/public/images/member2_banner.png) no-repeat;
  background-size: cover;
  position: absolute;
  bottom: -68rpx;
  display: flex;
  align-items: center;
}
.member_card .member_msg {
  display: flex;
  margin-left: 64rpx;
}
.member_card .member_msg .icon {
  width: 108rpx;
  height: 108rpx;
}
.member_card .member_msg .icon image {
  width: 100%;
}
.member_card .member_msg .member_login {
  margin-left: 18rpx;
}
.member_card .member_msg .member_login .member_login_top {
  display: flex;
  align-items: center;
  color: #7b5b21;
  font-size: 40rpx;
}
.member_card .member_msg .member_login .member_login_top > view:nth-child(2) {
  width: 106rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  color: #fbedc5;
  font-size: 28rpx;
  background-color: #a5a5a5;
  border-radius: 16rpx;
  margin-left: 8rpx;
}
.member_card .member_msg .member_login .member_login_top + view {
  color: #7b5b21;
  font-size: 28rpx;
  margin-top: 15rpx;
}
.mybuymember_warp {
  margin-top: 116rpx;
  position: relative;
}
.vippower_content {
  display: flex;
