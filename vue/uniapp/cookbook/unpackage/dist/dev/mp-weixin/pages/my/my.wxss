.header {
  width: 750rpx;
  height: 150rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  position: fixed;
  top: 0;
  z-index: 999;
}
.header .header_title {
  color: #000000;
  font-size: 36rpx;
  font-weight: 700;
  text-align: center;
  margin-bottom: 20rpx;
}
.banner {
  width: 750rpx;
  height: 324rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 150rpx;
}
.banner .banner_top {
  display: flex;
  width: 686rpx;
}
.banner .banner_top .top_left {
  width: 128rpx;
  height: 129rpx;
  border-radius: 65rpx;
}
.banner .banner_top .top_left .avatarUrl {
  width: 100%;
  border-radius: 65rpx;
}
.banner .banner_top .top_middle {
  padding-left: 18rpx;
  box-sizing: border-box;
}
.banner .banner_top .top_middle .middle_top {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.banner .banner_top .top_middle .middle_top .title {
  color: #515151;
  font-size: 30rpx;
}
.banner .banner_top .top_middle .middle_bottom {
  color: #d1d1d1;
  font-size: 22rpx;
  margin-top: 20rpx;
}
.banner .banner_top .top_right {
  margin-left: auto;
  display: flex;
  align-items: center;
}
.banner .banner_bottom {
  width: 686rpx;
  height: 118rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  margin-top: 34rpx;
}
.banner .banner_bottom .banner_bottom_msg {
  display: flex;
  justify-content: space-between;
  color: #b4853f;
  font-size: 22rpx;
  padding: 20rpx 100rpx;
  box-sizing: border-box;
}
.my_menu {
  width: 750rpx;
  height: 122rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.my_menu .menu_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-evenly;
  height: 122rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
}
.my_menu .menu_item text {
  color: #5a5a5a;
  font-size: 28rpx;
}
.tabs_warp {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.more {
  color: #5a5a5a;
  font-size: 22rpx;
  height: 72rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mymenu_warp {
  width: 100%;
}
.mymenu_warp .mymenu_title {
  color: #5a5a5a;
  font-size: 28rpx;
  margin: 10rpx 0 32rpx 32rpx;
}
.mymenu_warp .mymenu_scroll {
  margin-left: 30rpx;
}
.mymenu_warp .mymenu_scroll .mymenu_item {
  margin-right: 32rpx;
}
.operate_warp {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.operate_warp .operate_item {
  width: 686rpx;
  height: 95rpx;
  display: flex;
  align-items: center;
}
.operate_warp .operate_item .operate_msg {
  height: 95rpx;
  line-height: 95rpx;
  margin-left: 26rpx;
  color: #5a5a5a;
  font-size: 28rpx;
  flex: 1;
  border-bottom: 1rpx solid #dbdbdb;
}

