.content {
  width: 750rpx;
  background: linear-gradient(#2badff, #d1eeff, #f0faff);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.header {
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.search {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
.search .header_input {
  background-color: #f0faff;
  width: 95%;
  height: 50rpx;
  border-radius: 30rpx;
  padding-left: 100rpx;
  box-sizing: border-box;
}
.search .search_icon {
  height: 50rpx;
  width: 40rpx;
  position: absolute;
  top: 0;
  left: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.search .search_icon ._img {
  width: 90%;
}
.uni-input-placeholder {
  color: #ccc;
  font-size: 25rpx;
}
.banner {
  width: 95%;
  height: 300rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  background: url(/static/images/home_banner.jpg) no-repeat;
  background-size: cover;
}
.address {
  width: 95%;
  height: 200rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.address .text {
  width: 300rpx;
  height: 100rpx;
  margin-left: 180rpx;
  color: #fff;
  font-size: 25rpx;
  letter-spacing: 5rpx;
}
.address .text .text_title {
  font-size: 30rpx;
}
.address .address_search {
  width: 110rpx;
  height: 110rpx;
  background: url(data:image/png;base64,) no-repeat;
  margin-right: 20rpx;
}
.content_warp {
  width: 95%;
  margin-top: 20rpx;
}
.type_item_warp {
  width: 100%;
  border-bottom: 1rpx solid #fff;
  margin-bottom: 20rpx;
}
.type_item_warp .type_title_warp {
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.type_item_warp .type_title_warp .type_title {
  font-weight: 700;
}
.type_item_warp .type_title_warp .type_btn {
  width: 100rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 20rpx;
  font-size: 20rpx;
  color: #fff;
  background-color: #1da0f3;
}
.type_item_warp .type_content {
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.type_item_warp .type_content .type_item ._img {
  max-height: 130rpx;
}
.booklist {
  width: 100%;
  height: 800rpx;
}
.booklist .booklist_title {
  font-weight: 700;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.booklist .booklist_warp {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-top: 20rpx;
}
.booklist_item {
  width: 340rpx;
  height: 550rpx;
  background-color: #fff;
  border-radius: 20rpx;
  font-size: 20rpx;
  color: #ccc;
  line-height: 40rpx;
  margin-bottom: 20rpx;
}
.booklist_item .book_title {
  color: #333;
}
.booklist_item .shopping {
  background-color: #1da0f3;
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 25rpx;
  margin-left: auto;
  margin-right: 20rpx;
  margin-top: 20rpx;
}

